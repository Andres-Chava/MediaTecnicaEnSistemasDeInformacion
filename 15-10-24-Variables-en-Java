class HelloWorld {
    public static void main(String[] args) {
        // Enteras
        int entera;
        Integer entero;
        System.out.println("El rango de un entero es: 2,147,483,648 negativo a 2,147,483,647 positivo");
        System.out.println(" ");
        
        long enteroGrande;
        Long enteroGrandeObject;
        System.out.println("El rango de un long es: 9,223,372,036,854,775,808 negativo a 9,223,372,036,854,775,807 positivo.");
        System.out.println(" ");
        
        short enteroPequeno;
        Short enteroPequenoObject;
        System.out.println("El rango de un short es: 32,768 negativo a 32,767 positivo.");
        System.out.println(" ");
        
        byte enteroMuyPequeno;
        Byte enteroMuyPequenoObject;
        System.out.println("El rango de un byte es: 128 negativo a 127 positivo.");
        System.out.println(" ");
        
        
        // Decimales
        float decimal;
        Float decimalObject;
        System.out.println("El rango de un float es: 1.4e-045 a 3.4e+038.");
        System.out.println(" ");
        
        double decimal2;
        Double decimal2Object;
        System.out.println("El rango de un double es: 4.9e-324 a 1.8e+308.");
        System.out.println(" ");
        
        
        // Boolean
        boolean bool;
        Boolean booleanObject;
        System.out.println("El booleano puede ser Verdadero o Falso.");
        System.out.println(" ");
        
        
        // Cadena de caracteres
        String cadena;
        System.out.println("El string puede imprimir cadenas de palabras.");
        System.out.println(" ");
        
        
        // Caracter
        char caracter;
        System.out.println("El char puede tener un simbolo, una letra, o un valor numerico que represente una lentra en UniCode.");
        System.out.println(" ");
        
        
        // Object
        Object objeto;
        System.out.println("El objeto puede almacenar cualquier objeto, y este objeto puede tomar tipo de dato.");
        System.out.println(" ");
    
    }
}
